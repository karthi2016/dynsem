module Signatures

imports Common

templates

  ModuleSection.Signatures = <
    signatures
      <SignatureRule*; separator="\n">>

  SignatureRule.SignatureRule = <<ID> : <SignatureType> <SignatureAnnos?>>
  SignatureType.Type = [[ID*; separator=" * "] -> [ID]]
  SignatureType.Inj  = <<ID>>
  
  SignatureAnnos.SignatureAnnos = <{ <SignatureAnno+; separator=", "> }>
  SignatureAnno.ValueAnno = <value>
  SignatureAnno.NativeAnno = <native>
