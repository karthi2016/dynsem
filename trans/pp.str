module pp

imports
  libstratego-lib
  libstratego-gpp
  lib/runtime/tmpl/pp
  Common.pp.generated
  Module.pp.generated
  ds.pp.generated
  Lexical.pp.generated
  Signatures.pp.generated
  include/ds-parenthesize
  
rules
     
  pp-dynsem-string =
    parenthesize-ds
    ; prettyprint-Module
    ; !V([], <id>)
    ; box2text-string(|80)

  pp-debug :
    ast -> result
    with
       result := <pp-dynsem-string> ast
    <+ <bottomup(try(not(is-string); not(is-list); not(pp-dynsem-string); debug(!"cannot pp ")))> ast
    ;  result := ""
       
       